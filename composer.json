{
    "name": "aurimasniekis/doctrine-collection-updater",
    "type": "library",
    "description": "Doctrine Doctrine collection element updater",
    "keywords": ["doctrine-collection", "doctrine", "collection"],
    "homepage": "https://github.com/aurimasniekis/doctrine-collection-updater",
    "license": "MIT",
    "authors": [
        {
            "name": "Aurimas Niekis",
            "email": "aurimas@niekis.lt"
        }
    ],
    "require": {
        "php": "^7.0",
        "doctrine/dbal": ">=2.5 <3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.4"
    },
    "autoload": {
        "psr-4": {
            "AurimasNiekis\\DoctrineCollectionUpdater\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": { "AurimasNiekis\\DoctrineCollectionUpdater\\Test\\": "tests" }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit -c phpunit.xml.ci"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
